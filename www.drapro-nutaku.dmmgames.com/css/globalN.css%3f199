  50% {
    -webkit-transform: scale3d(0.85, 0.85, 1);
            transform: scale3d(0.85, 0.85, 1);
  }

  100% {
    -webkit-transform: scale3d(1, 1, 1);
            transform: scale3d(1, 1, 1);
  }
}

@keyframes pushAnim {
  0% {
    -webkit-transform: scale3d(1, 1, 1);
            transform: scale3d(1, 1, 1);
  }

  50% {
    -webkit-transform: scale3d(0.85, 0.85, 1);
            transform: scale3d(0.85, 0.85, 1);
  }

  100% {
    -webkit-transform: scale3d(1, 1, 1);
            transform: scale3d(1, 1, 1);
  }
}

