    display: block;
    top: 25px;
    -webkit-animation: popup-open 200ms ease-out;
            animation: popup-open 200ms ease-out;
}

@-webkit-keyframes popup-open {
    from {
        opacity: 0;
        -webkit-transform: scale(0.9);
                transform: scale(0.9);
    }
    99.9%, to {
        opacity: 1.0;
        -webkit-transform: scale(1);
                transform: scale(1);
    }
}

@keyframes popup-open {
    from {
        opacity: 0;
        -webkit-transform: scale(0.9);
                transform: scale(0.9);
    }
    99.9%, to {
        opacity: 1.0;
        -webkit-transform: scale(1);
                transform: scale(1);
    }
}

#stamp_popup.close {
    top: -1000px;
    display: inline;
    opacity: 0.0;
    -webkit-animation: popup-close 200ms ease-out;
            animation: popup-close 200ms ease-out;
}

@-webkit-keyframes popup-close {
    from {
        top: 25px;
        opacity: 1.0;
        -webkit-transform: scale(1);
                transform: scale(1);
    }
    99.9%, to {
        top: 25px;
        opacity: 0;
        -webkit-transform: scale(0.9);
                transform: scale(0.9);
    }
}

@keyframes popup-close {
    from {
        top: 25px;
        opacity: 1.0;
        -webkit-transform: scale(1);
                transform: scale(1);
    }
    99.9%, to {
        top: 25px;
        opacity: 0;
        -webkit-transform: scale(0.9);
                transform: scale(0.9);
    }
}

