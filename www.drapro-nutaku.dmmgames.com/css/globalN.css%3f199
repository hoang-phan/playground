/*なにか出したり消したりアニメーションさせたいときに使うべし*/

.fadeAnim {
    transition: all 0.2s ease-out;
    -webkit-transform: scale3d(1,1,1);
            transform: scale3d(1,1,1);
}

.fadeAnim.hide {
    opacity: 0;
    height: 0;
    -webkit-transform: scale3d(0,1,1);
            transform: scale3d(0,1,1);
}

/*ajax用ローディング*/

#ajaxLoadingImg{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.3);
    z-index: 3000;
}

#ajaxLoadingImg span{
    position: absolute;
    top: 200px;
    left: 0;
    right: 0;
    margin: 0 auto;
    width: 80px;
    height: 80px;
    border-radius: 80px;
    border: 4px dashed #fff;
    -webkit-transform: translate3d(0,0,0) rotate(0deg);
            transform: translate3d(0,0,0) rotate(0deg);
    -webkit-animation: roooool 1s ease-in-out 0s infinite;
            animation: roooool 1s ease-in-out 0s infinite;
}

@-webkit-keyframes roooool{
    from{
        -webkit-transform: translate3d(0,0,0) rotate(0deg);
                transform: translate3d(0,0,0) rotate(0deg);
    }
    99.9%, to{
        -webkit-transform: translate3d(0,0,0) rotate(360deg);
                transform: translate3d(0,0,0) rotate(360deg);
    }
}

@keyframes roooool{
    from{
        -webkit-transform: translate3d(0,0,0) rotate(0deg);
                transform: translate3d(0,0,0) rotate(0deg);
    }
    99.9%, to{
        -webkit-transform: translate3d(0,0,0) rotate(360deg);
                transform: translate3d(0,0,0) rotate(360deg);
    }
}

/*dom制御系*/

.showDis {
  display: block !important;
}

.hideDis {
  display: none !important;
}

.showOpa {
  opacity: 1 !important;
}

.hideOpa {
  opacity: 0 !important;
}

.showVis {
  visibility: visible !important;
}

.hideVis {
  visibility: hidden !important;
}

.cursorPointer {
  display: block !important;
}


/*見た目の変化ナシでクリック/タッチ制御*/

.unchangeDisabled {
  pointer-events: none;
}

/*強制再描画(常に描画させたい要素に入れるべし)*/

.redraw {
  -webkit-animation: redraw 1s linear 0s infinite;
          animation: redraw 1s linear 0s infinite;
}

@-webkit-keyframes redraw {
  0% {
    min-width: 1px;
  }

  100% {
    min-width: 2px;
  }
}

@keyframes redraw {
  0% {
    min-width: 1px;
  }

  100% {
    min-width: 2px;
  }
}
